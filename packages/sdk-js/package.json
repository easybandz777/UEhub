{
  "name": "@ue-hub/sdk-js",
  "version": "1.0.0",
  "description": "Generated JavaScript SDK for UE Hub API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "generate": "openapi-generator-cli generate -i ../backend/openapi.json -g typescript-fetch -o ./src/generated --additional-properties=typescriptThreePlus=true",
    "clean": "rm -rf dist src/generated"
  },
  "dependencies": {
    "@ue-hub/types": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "tsup": "^7.2.0",
    "@openapitools/openapi-generator-cli": "^2.7.0"
  }
}
