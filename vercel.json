{
  "version": 2,
  "name": "ue-hub-frontend",
  "buildCommand": "ls -la && ls -la frontend/ && cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "echo 'Checking directory structure' && ls -la",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://uehub.fly.dev/v1/:path*"
    }
  ]
}
