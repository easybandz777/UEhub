{
  "version": 2,
  "name": "ue-hub-frontend",
  "buildCommand": "cp package-vercel.json package.json && npm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "cp package-vercel.json package.json && npm install",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://uehub.fly.dev/v1/:path*"
    }
  ]
}
