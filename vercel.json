{
  "version": 2,
  "name": "ue-hub-frontend",
  "buildCommand": "npm --prefix frontend install && npm --prefix frontend run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "echo 'Skipping root install'",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://uehub.fly.dev/v1/:path*"
    }
  ]
}
