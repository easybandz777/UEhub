{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://uehub.fly.dev/v1/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
