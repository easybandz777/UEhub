{
  "version": 2,
  "name": "ue-hub-frontend",
  "buildCommand": "npm install && npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "rootDirectory": "frontend",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://uehub.fly.dev/v1/:path*"
    }
  ]
}
